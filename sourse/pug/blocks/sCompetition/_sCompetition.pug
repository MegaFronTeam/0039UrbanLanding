mixin sCompetition(data)
	// start sCompetition
	+b.SECTION.sCompetition.section#sCompetition&attributes(attributes)
		.container
			+e.inner
				+b.section-title
					h2 Пройдите конкурсный отбор
					
				+e.row
					mixin item(t, p) 
							+e.item.card
								.card__title!= t
								p!= p
								block
					+item(`Заявка и анкета`,`Ссылка на&nbsp;анкету придет на&nbsp;почту после заполнения заявки`)

					+item(`Тестирование`,`Ссылка на&nbsp;тестирование придет  на&nbsp;почту после проверки анкеты  на&nbsp;соответствие формальным критериям.`)
						.before
						p: +e.link(data-fancybox data-type="ajax"  href="modal/text.html") Узнать больше о&nbsp;критериях отбора
						+e.label Завершите этап  <br> до&nbsp;25.09.23

					+item(`Приглашение в&nbsp;программу`,`После успешного тестирования вы&nbsp;будете зачислены на&nbsp;программу &laquo;Урбан Лидер. СТАРТ raquo;`)
	// end sCompetition